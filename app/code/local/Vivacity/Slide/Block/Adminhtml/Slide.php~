<?php
class Responsive_Slide_Block_Adminhtml_Slide extends Mage_Adminhtml_Block_Widget_Grid_Container
{
  public function __construct()
  {
    $this->_controller = 'adminhtml_slide';
    $this->_blockGroup = 'slide';
    $this->_headerText = Mage::helper('slide')->__('Item Manager');
    $this->_addButtonLabel = Mage::helper('slide')->__('Add Item');
    parent::__construct();
  }
}
